

class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        // code here
        int max_profit=0;
        int buyprice=INT_MAX;
        for(int i=0;i<prices.size();i++){
            if(buyprice>prices[i]){
                buyprice=prices[i];
            }
            else{
                max_profit=max(max_profit,prices[i]-buyprice);
            }
        } 
        return max_profit;
    }
};
