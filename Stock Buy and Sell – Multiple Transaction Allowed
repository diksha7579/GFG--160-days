//Stock Buy and Sell â€“ Multiple Transaction Allowed//
    //Day-07 //


class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        // code here
        int n=prices.size();
        int max_profit=0;
        for(int i=0;i<n-1;i++)
        {
            if(prices[i]<prices[i+1])
            {
                max_profit+=prices[i+1]-prices[i];
            }
        }
        return max_profit;
    }
};
